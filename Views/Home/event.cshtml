                <div class="form_row"> <label>Category: </label>
                	<select name="loc" id="loc"><option>Select one...</option><option>concert</option><option>botique</option><option>book fair</option><option>art exibition</option></select>
                </div>
            
                    
                <div class="form_row"> 
                	<input type="submit" value="Search" name="submit" class="submit_btn_02" onclick="find()"/>
                </div>
            </div>
            
       
    </div><!-- end of section 1 -->
