@{
    ViewBag.Title = "event";
    Layout = "~/Views/Shared/layout.cshtml";
}
<head>

    <script language="JavaScript">

        function find() {

            var input = $('#place').val();
            $("#listv").empty();

         
            var userName = $('#loc').val();
          

            $.getJSON("/home/CheckUserName2?place=" + input + "&loc=" + userName, function (data) {
                $.each(data, function (i, item) {
                    var MyDate_String_Value = item.date;
                    var value = new Date
                       (
                            parseInt(MyDate_String_Value.replace(/(^.*\()|([+-].*$)/g, ''))
                       );
                    var dat = value.getMonth() +
                                             1 +
                                           "/" +
                               value.getDate() +
                                           "/" +
                           value.getFullYear();
                    $("#listv").append("<h4>" + item.name + " " + item.category + "--" + item.city + "--" + item.country + "--" + dat + "--" + item.phone + "</h4>");

                });

            });

        }
        function formSubmit(messageTag, formTag, detail) {
            var x = document.getElementById(messageTag);
            var y = x.getElementsByTagName('li');
            var z = document.getElementById(detail);
            if (x.style.visibility == 'hidden' && z.style.visibility == 'hidden') {
                x.style.visibility = 'visible'
                y[0].innerHTML = "event1";
                y[1].innerHTML = "event2";
                y[2].innerHTML = "event3";
                y[3].innerHTML = "event4";
                y[4].innerHTML = "event5";
                y[5].innerHTML = "event6";
                y[6].innerHTML = "event7";

                z.style.visibility = 'visible'
                document.forms[formTag].submit();
            }



        }

</script>

</head>


<body>

<div id="templatemo_content_panel_1">

